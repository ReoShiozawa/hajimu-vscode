{
  "Hello World": {
    "prefix": ["hello", "こんにちは"],
    "body": [
      "表示(\"こんにちは、世界！\")"
    ],
    "description": "Hello World を出力"
  },
  "変数宣言": {
    "prefix": ["変数", "var", "hensu"],
    "body": [
      "変数 ${1:名前} = ${2:値}"
    ],
    "description": "変数を宣言する"
  },
  "定数宣言": {
    "prefix": ["定数", "const", "teisu"],
    "body": [
      "定数 ${1:名前} = ${2:値}"
    ],
    "description": "定数を宣言する"
  },
  "関数定義": {
    "prefix": ["関数", "func", "kansu"],
    "body": [
      "関数 ${1:関数名}(${2:引数}):",
      "    ${3:// 処理}",
      "終わり"
    ],
    "description": "関数を定義する"
  },
  "関数（戻り値あり）": {
    "prefix": ["関数戻す", "funcr"],
    "body": [
      "関数 ${1:関数名}(${2:引数}):",
      "    ${3:// 処理}",
      "    戻す ${4:値}",
      "終わり"
    ],
    "description": "戻り値のある関数を定義する"
  },
  "もし（条件分岐）": {
    "prefix": ["もし", "if", "moshi"],
    "body": [
      "もし ${1:条件} なら",
      "    ${2:// 処理}",
      "終わり"
    ],
    "description": "条件分岐（if文）"
  },
  "もし・それ以外": {
    "prefix": ["もしそれ以外", "ifelse"],
    "body": [
      "もし ${1:条件} なら",
      "    ${2:// 真の処理}",
      "それ以外",
      "    ${3:// 偽の処理}",
      "終わり"
    ],
    "description": "条件分岐（if-else文）"
  },
  "もし・それ以外もし": {
    "prefix": ["もしそれ以外もし", "ifelseif"],
    "body": [
      "もし ${1:条件1} なら",
      "    ${2:// 処理1}",
      "それ以外もし ${3:条件2} なら",
      "    ${4:// 処理2}",
      "それ以外",
      "    ${5:// その他}",
      "終わり"
    ],
    "description": "条件分岐（if-else if-else文）"
  },
  "繰り返し（for）": {
    "prefix": ["繰り返す", "for", "kurikaesu"],
    "body": [
      "${1:i} を ${2:0} から ${3:9} 繰り返す",
      "    ${4:// 処理}",
      "終わり"
    ],
    "description": "カウンタ繰り返し（for文）"
  },
  "繰り返し（foreach）": {
    "prefix": ["各", "foreach", "kaku"],
    "body": [
      "${1:配列} を ${2:要素} で繰り返す",
      "    ${3:// 処理}",
      "終わり"
    ],
    "description": "要素繰り返し（foreach文）"
  },
  "条件繰り返し（while）": {
    "prefix": ["条件", "while", "joken"],
    "body": [
      "条件 ${1:条件} の間",
      "    ${2:// 処理}",
      "終わり"
    ],
    "description": "条件繰り返し（while文）"
  },
  "クラス定義": {
    "prefix": ["型", "class", "kata"],
    "body": [
      "型 ${1:クラス名}:",
      "    初期化(${2:引数}):",
      "        自分.${3:属性} = ${4:値}",
      "    終わり",
      "",
      "    関数 ${5:メソッド名}(${6:}):",
      "        ${7:// 処理}",
      "    終わり",
      "終わり"
    ],
    "description": "クラスを定義する"
  },
  "クラス継承": {
    "prefix": ["型継承", "extends"],
    "body": [
      "型 ${1:子クラス} 継承 ${2:親クラス}:",
      "    初期化(${3:引数}):",
      "        親.初期化(${4:})",
      "        自分.${5:属性} = ${6:値}",
      "    終わり",
      "終わり"
    ],
    "description": "クラスの継承"
  },
  "試行（try-catch）": {
    "prefix": ["試行", "try", "shikou"],
    "body": [
      "試行:",
      "    ${1:// 処理}",
      "捕獲 ${2:エラー}:",
      "    ${3:// エラー処理}",
      "終わり"
    ],
    "description": "例外処理（try-catch文）"
  },
  "試行（try-catch-finally）": {
    "prefix": ["試行最終", "tryfinally"],
    "body": [
      "試行:",
      "    ${1:// 処理}",
      "捕獲 ${2:エラー}:",
      "    ${3:// エラー処理}",
      "最終:",
      "    ${4:// 最終処理}",
      "終わり"
    ],
    "description": "例外処理（try-catch-finally文）"
  },
  "選択（switch）": {
    "prefix": ["選択", "switch", "sentaku"],
    "body": [
      "選択 ${1:値}:",
      "    場合 ${2:値1}:",
      "        ${3:// 処理1}",
      "    場合 ${4:値2}:",
      "        ${5:// 処理2}",
      "    既定:",
      "        ${6:// デフォルト処理}",
      "終わり"
    ],
    "description": "選択文（switch文）"
  },
  "配列": {
    "prefix": ["配列", "array", "hairetsu"],
    "body": [
      "変数 ${1:配列名} = [${2:要素1}, ${3:要素2}, ${4:要素3}]"
    ],
    "description": "配列を宣言する"
  },
  "辞書": {
    "prefix": ["辞書", "dict", "jisho"],
    "body": [
      "変数 ${1:辞書名} = {\"${2:キー1}\": ${3:値1}, \"${4:キー2}\": ${5:値2}}"
    ],
    "description": "辞書を宣言する"
  },
  "表示": {
    "prefix": ["表示", "print", "hyoji"],
    "body": [
      "表示(${1:\"メッセージ\"})"
    ],
    "description": "コンソールに出力する"
  },
  "入力": {
    "prefix": ["入力", "input", "nyuryoku"],
    "body": [
      "変数 ${1:値} = 入力(\"${2:プロンプト}: \")"
    ],
    "description": "ユーザー入力を受け取る"
  },
  "ファイル読み込み": {
    "prefix": ["読み込む", "readfile"],
    "body": [
      "変数 ${1:内容} = 読み込む(\"${2:ファイルパス}\")"
    ],
    "description": "ファイルを読み込む"
  },
  "ファイル書き込み": {
    "prefix": ["書き込む", "writefile"],
    "body": [
      "書き込む(\"${1:ファイルパス}\", ${2:内容})"
    ],
    "description": "ファイルに書き込む"
  },
  "HTTP GET": {
    "prefix": ["HTTP取得", "httpget"],
    "body": [
      "変数 ${1:応答} = HTTP取得(\"${2:URL}\")"
    ],
    "description": "HTTP GETリクエスト"
  },
  "HTTP POST": {
    "prefix": ["HTTP送信", "httppost"],
    "body": [
      "変数 ${1:応答} = HTTP送信(\"${2:URL}\", ${3:データ})"
    ],
    "description": "HTTP POSTリクエスト"
  },
  "パイプ": {
    "prefix": ["パイプ", "pipe"],
    "body": [
      "${1:値} |> ${2:関数}"
    ],
    "description": "パイプ演算子"
  },
  "文字列補間": {
    "prefix": ["補間", "interpolation"],
    "body": [
      "\"${1:テキスト}{${2:式}}${3:テキスト}\""
    ],
    "description": "文字列補間"
  },
  "変換（map）": {
    "prefix": ["変換", "map"],
    "body": [
      "変数 ${1:結果} = 変換(${2:配列}, 関数(${3:要素}):",
      "    戻す ${4:式}",
      "終わり)"
    ],
    "description": "配列を変換する（map）"
  },
  "抽出（filter）": {
    "prefix": ["抽出", "filter"],
    "body": [
      "変数 ${1:結果} = 抽出(${2:配列}, 関数(${3:要素}):",
      "    戻す ${4:条件}",
      "終わり)"
    ],
    "description": "配列を抽出する（filter）"
  },
  "集約（reduce）": {
    "prefix": ["集約", "reduce"],
    "body": [
      "変数 ${1:結果} = 集約(${2:配列}, ${3:初期値}, 関数(${4:累積}, ${5:要素}):",
      "    戻す ${6:式}",
      "終わり)"
    ],
    "description": "配列を集約する（reduce）"
  },
  "テスト": {
    "prefix": ["テスト", "test"],
    "body": [
      "テスト(\"${1:テスト名}\", 関数():",
      "    ${2:// テスト処理}",
      "    期待(${3:実際値}, ${4:期待値})",
      "終わり)",
      "",
      "テスト実行()"
    ],
    "description": "テストケースを作成する"
  },
  "非同期処理": {
    "prefix": ["非同期", "async"],
    "body": [
      "変数 ${1:タスク} = 非同期実行(関数():",
      "    ${2:// 非同期処理}",
      "終わり)",
      "変数 ${3:結果} = 待機(${1:タスク})"
    ],
    "description": "非同期処理を実行する"
  },
  "列挙型": {
    "prefix": ["列挙", "enum"],
    "body": [
      "列挙 ${1:名前}:",
      "    ${2:値1}",
      "    ${3:値2}",
      "    ${4:値3}",
      "終わり"
    ],
    "description": "列挙型を定義する"
  },
  "正規表現": {
    "prefix": ["正規", "regex"],
    "body": [
      "変数 ${1:結果} = 正規一致(${2:文字列}, \"${3:パターン}\")"
    ],
    "description": "正規表現でマッチング"
  },
  "JSONパース": {
    "prefix": ["JSON解析", "jsonparse"],
    "body": [
      "変数 ${1:データ} = JSON解析(${2:文字列})"
    ],
    "description": "JSON文字列をパースする"
  },
  "取り込む": {
    "prefix": ["取り込む", "import"],
    "body": [
      "取り込む(\"${1:モジュールパス}\")"
    ],
    "description": "モジュールを取り込む"
  }
}
